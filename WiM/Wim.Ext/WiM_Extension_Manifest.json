{
  "manifestVersion": 1,
  "id": "WimDevOpExtension",
  "version": "0.0.27",
  "name": "WorkitemManagerExtension",
  "baseUri": "https://localhost:44367",
  "publisher": "bandik",
  "description": " A hub for adding multiple tasks at once (beta)",
  "public": false,
  "categories": [ "Plan and track" ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "images/tree1.ico"
  },
  "contributions": [
    {
      "id": "Bandik.WimDevOpExtension.beta",
      "type": "ms.vss-web.hub",
      "description": "Adds a 'WiM Extension Beta hub to the Work hub group. (beta)",
      "targets": [
        "ms.vss-work-web.work-hub-group"
      ],
      "properties": {
        "name": "WiM",
        "order": 99,
        "uri": "Test.html",
        "iconAsset": "bandik.WimDevOpExtension/images/icon.png",
        "_sharedData": {
          "assets": [
            "bandik.WimDevOpExtension/images/icon.png"
          ]
        }
      }
    },
    {
      "id": "Bandik.WimDevOpExtension.teamsettings-form",
      "type": "ms.vss-web.control",
      "description": "The content to be displayed in the Team Settings dialog",
      "targets": [],
      "properties": {
        "uri": "teamsettings-form.html"
      }
    },
    {
      "id": "Bandik.WimDevOpExtension.manage-teams",
      "type": "ms.vss-web.control",
      "description": "The content to be displayed in the Manage Teams dialog",
      "targets": [],
      "properties": {
        "uri": "manage-teams.html"
      }
    },
    {
      "id": "Bandik.WimDevOpExtension.manage-tasks",
      "type": "ms.vss-web.control",
      "description": "The content to be displayed in the Manage Tasks dialog",
      "targets": [],
      "properties": {
        "uri": "manage-tasks.html"
      }
    }
  ],
  "scopes": [
    "vso.work"
  ],
  "files": [
    {
      "path": "Test.html",
      "addressable": true
    },
    {
      "path": "teamsettings-form.html",
      "addressable": true
    },
    {
      "path": "SDK/scripts",
      "addressable": true
    },
    {
      "path": "scripts",
      "addressable": true
    },
    {
      "path": "images/logoHierarchical.png",
      "addressable": true
    },
    {
      "path": "images/tree1_128.png",
      "addressable": true
    },
    {
      "path": "images/icon.png",
      "addressable": true
    }

  ]
}